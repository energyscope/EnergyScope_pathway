			sum {t in PERIODS, h in HOUR_OF_PERIOD[t], td in TYPICAL_DAY_OF_PERIOD[t]} (F_t [y,tech,h,td] * t_op[h,td]) / 8760 / max(F[y,tech],0.0001)
			,c_p[y,tech],tau[y,tech],GWP_constr[y,tech] >> (PathName & "/assets.txt"); 
