			# Electrolysis
			if sum{t in PERIODS, h in HOUR_OF_PERIOD[t], td in TYPICAL_DAY_OF_PERIOD[t]}(F_t [y,"H2_ELECTROLYSIS", h, td]  ) > 10 then {
