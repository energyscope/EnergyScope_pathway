					TYPICAL_DAY_OF_PERIOD[t]}(F_t [y,"H2_RE", h, td] * t_op [h, td]) / 1000 , "H2", "#FF00FF", "TWh" > 
					(PathName & "/input2sankey.csv");
			if F [y,"H2_STORAGE"] > 0.001 then { 
				printf "%s,%s,%.2f,%s,%s,%s\n", "H2 prod" , "H2 sto.", sum{t in PERIODS, h in HOUR_OF_PERIOD[t], td in 
					TYPICAL_DAY_OF_PERIOD[t]}(Storage_in [y,"H2_STORAGE", "H2", h, td]  ) / 1000 , 
					"H2", "#FF00FF", "TWh" > (PathName & "/input2sankey.csv");
				printf "%s,%s,%.2f,%s,%s,%s\n", "H2 sto." , "H2", sum{t in PERIODS, h in HOUR_OF_PERIOD[t], td in 
					TYPICAL_DAY_OF_PERIOD[t]}(Storage_out [y,"H2_STORAGE", "H2", h, td]*storage_eff_out [y,"H2_STORAGE","H2"]  ) / 1000 , 
					"H2", "#FF00FF", "TWh" > (PathName & "/input2sankey.csv");
				printf "%s,%s,%.2f,%s,%s,%s\n", "H2 prod" , "H2", sum{t in PERIODS, h in HOUR_OF_PERIOD[t], td in 
					TYPICAL_DAY_OF_PERIOD[t]}( layers_in_out[y,"SMR","H2"] * F_t [y,"SMR", h, td] + layers_in_out[y,"H2_BIOMASS","H2"] * F_t [y,"H2_BIOMASS", h, td] + layers_in_out[y,"H2_ELECTROLYSIS","H2"] * F_t [y,"H2_ELECTROLYSIS", h, td]
